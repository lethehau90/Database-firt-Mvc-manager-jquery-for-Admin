@model IList<Web_config_v1.Models.Entity.Advertise>
@{
    string language =  Web_config_v1.Models.Service.DataCommon.GetSessionLanguage(Session);
}
@if (Model.Count > 0)
{
    <div id="logo">
        <a href="/" title="@Model.FirstOrDefault().Name">
            <img src="@Model.FirstOrDefault().Image">
        </a>
    </div>
}